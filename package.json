    "@commitlint/config-conventional": "^20.3.1",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "~5.2.3",
    "@types/mocha": "~10.0.10",
    "@types/ms": "~2.1.0",
    "@types/node": "^25.0.8",
